{
  "name": "milochau.cv.client",
  "version": "0.1.0",
  "description": "Milochau CV Client",
  "author": "Antoine Milochau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amilochau/aws-cv"
  },
  "homepage": "https://github.com/amilochau/aws-cv#readme",
  "bugs": {
    "url": "https://github.com/amilochau/aws-cv/issues"
  },
  "funding": "https://github.com/sponsors/amilochau",
  "company": "Milochau",
  "copyright": "Copyright Â© Milochau",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite serve --port 3000",
    "preview": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@mdi/js": "^7.2.96",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.13",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.3.2",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "eslint": "^8.40.0",
    "eslint-plugin-vue": "^9.13.0",
    "pinia": "^2.1.3",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-fonts": "^0.7.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.2.47",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.1",
    "vue-tsc": "~1.2.0",
    "vuetify": "^3.3.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/vue3-recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "rules": {
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  }
}
